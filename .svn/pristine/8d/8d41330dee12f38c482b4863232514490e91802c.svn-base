  }
  .el-submenu.is-active .el-submenu__title{
    border: none!important;
  }
  /*导航栏最小宽度120*/
  .el-submenu .el-menu-item{
    min-width: 100px!important;
  }
  /* 导航栏修改 end */
  
  /* 工作台修改搜索门店 */
  #selArea{
    border: none!important;
    width: 120px!important;
    text-align: center;
  }
  #selAreaForm > .el-form-item{
    line-height: 1px;
    margin-left:30px;
    margin-top: 20px;
    text-align: center;
  }
  #selAreaForm .el-form-item__label{
    font-size: 18px!important;
  }
  .el-date-editor.el-input.el-input--prefix.el-input--suffix.el-date-editor--date .el-input__inner{
    position: relative;
    top:-2px;
  }
  #searchContentBox .searchContent .el-input__inner{
    border: none;
    background-color:$globalColor!important;
    font-size: 14px!important;
    padding: 0 30px!important;
    color:#ffffff;
    
  }
  #searchContentBox .searchContent .el-input__inner::-webkit-input-placeholder{
      color:#ffffff;
  }
  #searchContentBox .searchContent .el-input__inner::-moz-placeholder {
      color:#ffffff;
  }
  #searchContentBox .searchContent .el-input__inner::-ms-placeholder {
    color:#ffffff;
  }
  .main-header-menus .el-submenu__icon-arrow.el-icon-arrow-down{
    color: #ffffff;
  }
  
  /* 修改搜索门店 */
  
  /*动画 */
  .fade-enter-active, .fade-leave-active {
    transition: opacity .5s
  }
  .fade-enter, .fade-leave-to /* .fade-leave-active in below version 2.1.8 */ {
    opacity: 0
  }
  
  
  .slide-fade-enter-active {
    transition: all .3s ease;
  }
  .slide-fade-leave-active {
    transition: all .8s cubic-bezier(1.0, 0.5, 0.8, 1.0);
  }
  .slide-fade-enter, .slide-fade-leave-to
  /* .slide-fade-leave-active for below version 2.1.8 */ {
    transform: translateX(10px);
    opacity: 0;
  }
  
  .bounce-enter-active {
    animation: bounce-in .5s;
  }
  .bounce-leave-active {
    animation: bounce-in .5s reverse;
  }
  @keyframes bounce-in {
    0% {
      transform: scale(0);
    }
    50% {
      transform: scale(1.5);
    }
    100% {
      transform: scale(1);
    }
  }
  /*动画  end*/
  #dialogNotice .el-dialog > .el-dialog__body{
    padding: 0 20px 30px 20px!important;
  }
  
  .el-radio__inner{
    border-radius:0!important;
  }
  
  // userDetails 用户评星
  #rateBox .el-rate__item .el-rate__icon{
    margin-right:3px!important;
  }
    // 菜单切换
  .userDetailRight .rightBox {
    & .el-radio-button:last-child .el-radio-button__inner{
      border-radius:0;
    }
    & .el-radio-button:first-child .el-radio-button__inner{
      border-left:1px solid #409EFF;
      border-radius:0;
    }
    & .el-radio-button__inner{
      border: 1px solid #409EFF;
      border-left: 0;
    }
  }
#cut_date .el-radio-button--small .el-radio-button__inner{
    padding: 7px 5px;
    border: none;
    border-radius: 0;
    color: #808088;
    user-select: none;
}
#cut_date .el-radio-button__orig-radio:checked+.el-radio-button__inner{
