import Vue from 'vue';
Vue.directive('focus',{
    inserted:function (el,binding) {
        // 获取焦点
        el.focus()
    }
})
Vue.directive('slimScroll',{
        inserted: function (el,binding) {
            $(el).slimScroll({
                width: '100%',
                height: '100%',
                color: '#4198FF',
                railColor: '#F2F2F2',
                railVisible: true,
                alwaysVisible: true
                });
          }
})