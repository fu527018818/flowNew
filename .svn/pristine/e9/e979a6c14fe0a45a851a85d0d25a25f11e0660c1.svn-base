.app{
    width: 100%;
    height: 100%;
}
.contentBox{
    width: 100%;
    background-color: #f2f2f2;
    position: relative;
}
.contentBox_child{
    width: 100%;
    padding-bottom:60px;
}
.contentFooter{
    height: 45px;
    width: 100%;
    background-color:#3987e6;
    &.isMd6{
        margin-top: 6px;
    }
}
.content{
    width:1024px;
    margin: 0 auto;
    padding-top: 6px;
    position: relative;
    min-height: 800px;
}
#cut_date .el-radio-button--small .el-radio-button__inner{
    padding: 7px 5px;
    border: none;
    border-radius: 0;
    color: #808088;
    user-select: none;
}
#cut_date .el-radio-button__orig-radio:checked+.el-radio-button__inner{
    color: #fff!important;
}
#turnoverCut .el-radio-button__inner {
    border:1px solid #409EFF;
    border-radius: 0;
    border-left:0!important;
    padding: 11px 32px;
}
#userCut .el-radio-button__inner{
    padding: 11px 31px;
}
#turnoverCut .el-radio-button:first-child .el-radio-button__inner{
        border-left: 1px solid #409EFF!important;
}
.el-date-editor .el-range-input, .el-date-editor .el-range-separator{
    font-size: 12px;
}
.el-date-editor .el-range__close-icon{
    position: absolute;
    right: 0;
    top: 50%;
    transform: translate(0,-50%);
}
/*客流滞留分析*/
#changSeparator > .el-date-editor .el-range-separator{
    width:15%!important;
}
#changSeparator > .el-date-editor .el-range-input{
    width: 34%!important;
}
/*客流滞留分析*/
#indent{
    height: 30px!important;
    border-radius: 0;
    padding: 0;
    text-indent: 5px;
}
#indent + .el-input__suffix{
    right: 0!important;
}
  .el-table .headerTr{
      background-color: #e5f1ff;
  }
  #editNotice .el-radio-button__inner{
    padding: 12px 50px!important;
    border:1px solid #409EFF;
    border-radius: 0;
    border-left:0!important;
}
#editNotice .el-radio-button:first-child .el-radio-button__inner{
    border-left: 1px solid #409EFF!important;
}
/*客户详情页编辑*/
.twoFont .el-form-item__label{
    text-align: left!important;
    padding:0 0 0 12px!important;
    letter-spacing: 20px!important;
    
}
.threeFont .el-form-item__label{
    text-align: left!important;
    padding:0 0 0 12px!important;
    letter-spacing: 5px!important;
}
/*客户详情页编辑*/
/*选择门店*/
#selectShop{
    width:300px!important;
}
/*选择门店*/

/*公告*/
#dialogNotice  .el-collapse-item__header{
    font-size: 14px!important;
}
#dialogNotice .el-dialog{
    max-width:1024px!important;
}
/*公告*/
/* notification 消息提示*/
.el-notification{
    background-color: #000000;
    width: 350px;
    opacity: 0.9;
}

/*消息提示end*/
/*表格 start*/
.el-table th{
    text-align: center;
    padding: 3px 0;
}
.el-table td{
    text-align: center;
    padding: 8px 0;
}

.userTraitStyle{
    text-align: left!important;
}

/*筛选按钮 start*/
#searchBtn{
    font-family: MicrosoftYaHeiLight;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0px;
    cursor: pointer;
    user-select: none;
    & .el-button{
        padding: 7px 20px;
        border-radius: 3px;
    }
        & > div:nth-child(1){
            color: #808080;
            font-size: 14px;
            display: inline-block;
            margin: 0 10px;
        }
}
/*筛选按钮 end*/

#dataEntering{
    position: absolute;
    right:20px;
    top: 50%;
    transform: translate(0,-50%);
    padding: 10px 10px;
	border-radius: 2px;
}
