.listTit{
    & >div:nth-child(1){
        font-family: MicrosoftYaHeiLight;
        font-size: 14px;
        font-weight: normal;
        font-stretch: normal;
        line-height: 26px;
        letter-spacing: 0px;
        color: #4c4c4c;
        margin: 38px 0 0 29px;
    }
}
.listDec{
    font-family: MicrosoftYaHeiLight;
    font-size: 14px;
    line-height: 40px;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0px;
    color: #808080;
    text-align: left;
    & div{
         margin-bottom: 20px;
         &:last-child{margin-bottom: 0;}
    }
}
.listDown{
    & > div{
        width: 150px;
        height: 40px;
        background-color: #4198ff;
        text-align: center;
        line-height: 40px;
        border-radius: 5px;
        color: #ffffff;
        cursor: pointer;
        text-align: center;
        margin:0 auto 20px 0;
        &:last-child{margin-bottom: 0;}
    }
}